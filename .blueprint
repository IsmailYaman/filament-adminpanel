created:
    - database/factories/ConferenceFactory.php
    - database/factories/VenueFactory.php
    - database/factories/SpeakerFactory.php
    - database/factories/TalkFactory.php
    - database/migrations/2025_08_12_190939_create_conferences_table.php
    - database/migrations/2025_08_12_190940_create_venues_table.php
    - database/migrations/2025_08_12_190941_create_speakers_table.php
    - database/migrations/2025_08_12_190942_create_talks_table.php
    - database/migrations/2025_08_12_190943_create_conference_speaker_table.php
    - database/migrations/2025_08_12_190944_create_conference_talk_table.php
    - app/Models/Conference.php
    - app/Models/Venue.php
    - app/Models/Speaker.php
    - app/Models/Talk.php
models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable', password: string, remember_token: 'string:100 nullable' }
    Conference: { name: string, description: string, start_date: date, end_date: date, status: string, region: string, venue_id: 'uniqueInteger nullable', relationships: { belongsTo: Venue, belongsToMany: 'Speaker, Talk' } }
    Venue: { name: string, city: string, country: string, postal_code: string, relationships: { hasMany: Conference } }
    Speaker: { name: string, email: string, bio: string, twitter_handle: string, relationships: { belongsToMany: Conference } }
    Talk: { title: string, abstract: string, speaker_id: unsignedInteger, relationships: { belongsTo: Speaker, belongsToMany: Conference } }
